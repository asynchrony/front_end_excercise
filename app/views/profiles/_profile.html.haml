%tr
  %td
    = profile.first_name
  %td
    = profile.last_name
  %td
    = profile.email
  %td
    = profile.gender
  %td
    = profile.income
  %td
    = profile.date_of_birth
  - if profile.location
    %td
      = profile.location.street
    %td
      = profile.location.city
    %td
      = profile.location.state
    %td
      = profile.location.zip
    %td
      = profile.location.country
  %td
    = profile.created_at
